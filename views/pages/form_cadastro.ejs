<!DOCTYPE html>
<html lang="en">

    <%- include('../partials/head'); %>

<body>

    <%- include('../partials/header'); %>

    <div class="container px-2 py-0">
        <h2 class="pb-2 border-bottom">Cadastro</h2>
   


    <div class="row">
        <div class="form-group col-md-3">
            <label for="codigo_ori">Codigo</label>
            <input type="text" class="form-control" id='fm-inp-codigo'>
        </div>

        <div class="form-group col-md-3">
            <label for="campo_nome">Nome</label>
            <input type="text" class="form-control" id='fm-inp-nome'>
        </div>

        <div class="form-group col-md-2">
            <label for="campo_fabricante">Fabricante</label>
            <input type="text" class="form-control" id='fm-inp-fabricante'>
        </div>
    </div>

    <div class="row">
        <div class="form-group col-md-5">
            <label for="campo_modelo">Modelo</label>
            <input type="text" class="form-control" id='fm-inp-modelo'>
        </div>


        <div class="form-group col-md-3">
            <label for="campo_linha">Linha</label>
            <select class="form-control" id="fm-inp-linha" required>
                <option selected disabled value="">None</option>
                <option value="continuada">Continuada</option>
                <option value="descontinuada">Descontinuada</option>
            </select>
            <div class="invalid-feedback">
                Please select a valid state.
            </div>
        </div>

    </div>

    <div class="row">
        <div class="form-group col-md-2">
            <br>
            <button class="form-control" type="button" onclick=addPecas_ori()>
                CADASTRAR</button>
        </div>

    </div>
        
</div>

<%- include('../partials/footer'); %>

<%- include('../partials/script');%>

</body>

</html>